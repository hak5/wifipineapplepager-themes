            { "text": "Clients", "x": 10, "y": 30, "text_color_palette": "black"  },
            { "image_path": "assets/settings/listhighlight.png", "x": 8, "y": 28},
			{ "image_path": "assets/slider.png", "x": 127, "y": 139},

            { "text": "Click to view\n\nconnected clients", "x": 196, "y": 30, "text_color_palette": "gray", "center_text_within": { "draw_bounds": false, "start_x": 196, "end_x": 430, "start_y": 30, "end_y": 180 }}
          ],
          "x": 0,
          "y": 0,
          "target": "pineap_clients_list",
          "variable": "$_PINEAPPLE_CLIENTS"
        }
